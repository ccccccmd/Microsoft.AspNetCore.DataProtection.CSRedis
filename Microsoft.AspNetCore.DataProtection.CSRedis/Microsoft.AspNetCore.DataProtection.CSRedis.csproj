<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Description>Support for storing data protection keys in CSRedis.</Description>
        <TargetFramework>netstandard2.1</TargetFramework>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageTags>aspnetcore;dataprotection;redis</PackageTags>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Caching.CSRedis" Version="3.6.5" />
      <PackageReference Include="Microsoft.AspNetCore.DataProtection" Version="3.1.7" />
    </ItemGroup>
    
</Project>
